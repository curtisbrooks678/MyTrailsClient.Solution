<h2>View all Journal Entries</h2>
<hr />

@if(Model.Count == 0)
{
  <p>You currently have no journal entries.</p>
}
<ul>
  @foreach(VisitEntry visitEntry in Model)
  {
    <li>@Html.ActionLink($"{visitEntry.VisitDate}", "Details", new { id = visitEntry.VisitEntryId })</li>
  }
</ul>

<p>@Html.ActionLink("Add a new Visit Entry", "Create")</p>